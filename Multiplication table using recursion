#include <iostream>

using namespace std;

int mul(int,int);
int main()
{
   int i=1,n;
   cout<<"Enter the number whose multiplication table you want to print: ";
   cin>>n;
  mul(n,i);
   
}


int mul(int n, int i)
{
    if (i<=10){
        cout<<n*i<<"\n";
        return (mul(n, i+1));
    }
    else 
    {
        return (1);
    }
}
